  body
    .layer4
      .page
        .titles-container
          #titles-information.titles
            #information-side-title.side-title.animonload1 Information
          #titles-projects.titles
            #projects-side-title.side-title.animonload1 Projects
          #titles-nekovault.titles
            #nekovault-side-title.side-title.animonload1 Neko&nbsp;Vault
            #titles-connector-1.titles
          #titles-contact.titles
            #contact-side-title.side-title.animonload1 Contact
